{
  "author": "Peter T Bosse II <ptb@ioutime.com> (http://ptb2.me/)",
  "devDependencies": {
    "npm-run-all": "4.1.5",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "rollup": "1.10.0",
    "rollup-plugin-commonjs": "9.3.4",
    "rollup-plugin-node-resolve": "4.2.3",
    "rollup-plugin-replace": "2.2.0",
    "terser": "3.17.0"
  },
  "license": "MIT",
  "main": "index.mjs",
  "module": "index.mjs",
  "scripts": {
    "build": "run-s build:rollup build:terser build:header build:chomp clean",
    "build:chomp": "perl -pi -e 'chomp if eof' index.mjs",
    "build:header": "sed -i '' -e '1s;^;/*! @copyright Facebook, Inc. | @license MIT | @link github.com/facebook/react | @version 16.6.3 *//* eslint-disable */;' index.mjs",
    "build:rollup": "rollup -c",
    "build:terser": "terser -c -m -o index.mjs --toplevel index.mjs",
    "clean": "rm -rf node_modules yarn.lock"
  }
}
