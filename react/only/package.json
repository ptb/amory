{
  "author": "Peter T Bosse II <ptb@ioutime.com> (http://ptb2.me/)",
  "devDependencies": {
    "npm-run-all": "4.1.3",
    "react": "16.5.2",
    "rollup": "0.66.5",
    "rollup-plugin-commonjs": "9.1.8",
    "rollup-plugin-node-resolve": "3.4.0",
    "rollup-plugin-replace": "2.1.0",
    "terser": "3.10.0"
  },
  "license": "MIT",
  "module": "index.js",
  "scripts": {
    "build": "run-s build:rollup build:terser build:header build:chomp clean",
    "build:chomp": "perl -pi -e 'chomp if eof' index.js",
    "build:header": "sed -i '' -e '1s;^;/*! @copyright Facebook, Inc. | @license MIT | @link github.com/facebook/react | @version 16.5.2 */;' index.js",
    "build:rollup": "rollup -c",
    "build:terser": "terser -c -m -o index.js --toplevel index.js",
    "clean": "rm -rf node_modules yarn.lock"
  }
}
