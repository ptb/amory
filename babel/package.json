{
  "author": "Peter T Bosse II <ptb@ioutime.com> (http://ptb2.me/)",
  "babel": {
    "plugins": [
      "@babel/plugin-syntax-dynamic-import",
      [
        "@babel/plugin-transform-runtime",
        {
          "useBuiltIns": true,
          "useESModules": true
        }
      ]
    ],
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false,
          "shippedProposals": true,
          "spec": true,
          "targets": {
            "esmodules": true
          },
          "useBuiltIns": "usage"
        }
      ]
    ]
  },
  "dependencies": {
    "@amory/merge": "latest",
    "@babel/core": "latest",
    "@babel/node": "latest",
    "@babel/plugin-syntax-dynamic-import": "latest",
    "@babel/plugin-transform-runtime": "latest",
    "@babel/preset-env": "latest",
    "@babel/runtime": "latest",
    "babel-loader": "next",
    "webpack": "4.17.2"
  },
  "engines": {
    "node": ">=8"
  },
  "license": "(Apache-2.0 OR MIT)",
  "main": "index.js",
  "name": "@amory/babel",
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/ptb/amory/tree/master/babel",
  "version": "2018.8.16-1"
}
