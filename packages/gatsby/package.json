{
  "author": "Peter T Bosse II <ptb@ioutime.com> (http://ptb2.me/)",
  "dependencies": {
    "chokidar-cli": "^1.x",
    "gatsby": "^1.x"
  },
  "license": "Apache-2.0",
  "name": "@ptb/gatsby",
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/ptb/amory/tree/master/packages/gatsby",
  "scripts": {
    "postinstall": "npx npm-run-all 'postinstall:*'",
    "postinstall:merge": "find '.amory' -maxdepth 1 -name '*.json' -exec npx -p @ptb/merge-json -c \"merge-json ../../../$(basename '{}') '{}'\" \\;",
    "postinstall:setup": "find '.amory' -maxdepth 1 -not -name '.amory' -not -name '*.json' -exec cp -R '{}' '../../../' \\;"
  },
  "version": "2018.3.25-29"
}
