{
  "authors": [
    "Kyle Mathews <mathews.kyle@gmail.com>",
    "Peter T Bosse II <ptb@ioutime.com> (http://ptb2.me/)"
  ],
  "babel": {
    "presets": [
      "env"
    ]
  },
  "bugs": {
    "url": "https://github.com/ptb/amory/issues"
  },
  "dependencies": {
    "@ptb/gatsby-plugin-styletron": "latest",
    "gatsby-plugin-sharp": "^1",
    "gatsby-source-filesystem": "^1",
    "gatsby-transformer-sharp": "^1"
  },
  "description": "Lazy-loading React image component with optional support for the blur-up effect.",
  "devDependencies": {
    "babel-cli": "^6",
    "babel-preset-env": "latest",
    "cross-env": "^5"
  },
  "homepage": "https://github.com/gatsbyjs/gatsby/tree/master/packages/gatsby-image#readme",
  "keywords": [
    "gatsby",
    "gatsby-component",
    "react-component"
  ],
  "license": "Apache-2.0",
  "main": "index.js",
  "name": "@ptb/gatsby-image",
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/ptb/amory/tree/master/packages/gatsby-image",
  "scripts": {
    "build": "babel src/index.js --out-file index.js --ignore __tests__",
    "postinstall": "npx npm-run-all 'postinstall:*'",
    "postinstall:merge": "find '.amory' -maxdepth 1 -name '*.json' -print0 | xargs -0 -I {} -n1 basename '{}' | xargs -I {} -n1 npx -p @ptb/merge-json -c 'merge-json ../../../{} .amory/{}'",
    "postinstall:setup": "find '.amory' -maxdepth 1 -not -name '.amory' -not -name '*.json' -exec cp -R '{}' '../../../' \\;",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src/index.js --out-file index.js --ignore __tests__"
  },
  "version": "2018.4.4-10"
}
