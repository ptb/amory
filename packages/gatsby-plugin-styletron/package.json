{
  "authors": [
    "Nadiia Dmytrenko <nadiia.dmytrenko@gmail.com>",
    "Peter T Bosse II <ptb@ioutime.com> (http://ptb2.me/)"
  ],
  "bugs": {
    "url": "https://github.com/ptb/amory/issues"
  },
  "dependencies": {
    "styletron-engine-atomic": "^1",
    "styletron-react": "^4",
    "styletron-react-core": "^1",
    "styletron-standard": "^1"
  },
  "description": "A Gatsby plugin for styletron with built-in server-side rendering support",
  "homepage": "https://github.com/ptb/amory/tree/master/packages/gatsby-plugin-styletron#readme",
  "keywords": [
    "gatsby",
    "gatsby-plugin"
  ],
  "license": "Apache-2.0",
  "main": "index.js",
  "name": "@ptb/gatsby-plugin-styletron",
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/ptb/amory/tree/master/packages/gatsby-plugin-styletron",
  "scripts": {
    "postinstall": "npx npm-run-all 'postinstall:*'",
    "postinstall:merge": "find '.amory' -maxdepth 1 -name '*.json' -print0 | xargs -0 -I {} -n1 basename '{}' | xargs -I {} -n1 npx -p @ptb/merge-json -c 'merge-json ../../../{} .amory/{}'",
    "postinstall:setup": "find '.amory' -maxdepth 1 -not -name '.amory' -not -name '*.json' -exec cp -R '{}' '../../../' \\;"
  },
  "version": "2018.4.29-0"
}
