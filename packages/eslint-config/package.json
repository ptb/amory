{
  "author": "Peter T Bosse II <ptb@ioutime.com> (http://ptb2.me/)",
  "dependencies": {
    "babel-eslint": "^8",
    "chokidar-cli": "^1",
    "eslint": "^4",
    "eslint-plugin-better": "latest",
    "eslint-plugin-fp": "^2",
    "eslint-plugin-import": "^2",
    "eslint-plugin-json": "^1",
    "eslint-plugin-jsx-a11y": "^6",
    "eslint-plugin-promise": "^3",
    "eslint-plugin-react": "^7",
    "eslint-plugin-standard": "^3"
  },
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "license": "Apache-2.0",
  "main": ".eslintrc.js",
  "name": "@ptb/eslint-config",
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/ptb/amory/tree/master/packages/eslint-config",
  "scripts": {
    "postinstall": "npx npm-run-all 'postinstall:*'",
    "postinstall:merge": "find '.amory' -maxdepth 1 -name '*.json' -print0 | xargs -0 -I {} -n1 basename '{}' | xargs -I {} -n1 npx -p @ptb/merge-json -c 'merge-json ../../../{} .amory/{}'"
  },
  "version": "2018.4.28-3"
}
