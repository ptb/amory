{
  "plugins": [
    "fp"
  ],
  "rules": {
    "fp/no-arguments": "off",
    "fp/no-class": "off",
    "fp/no-delete": "off",
    "fp/no-events": "off",
    "fp/no-get-set": "off",
    "fp/no-let": "off",
    "fp/no-loops": "off",
    "fp/no-mutating-assign": "off",
    "fp/no-mutating-methods": "off",
    "fp/no-mutation": [
      "off",
      {
        "commonjs": true
      }
    ],
    "fp/no-nil": "off",
    "fp/no-proxy": "off",
    "fp/no-rest-parameters": "off",
    "fp/no-this": "off",
    "fp/no-throw": "off",
    "fp/no-unused-expression": [
      "off",
      {
        "allowUseStrict": true
      }
    ],
    "fp/no-valueof-field": "off"
  }
}
